name=libkgsl
version=2.0.1-20111021
release=1
source=(libkgsl-imx_2.0.1-20111021_armhf.deb)

build() {
  ar x libkgsl-imx_2.0.1-20111021_armhf.deb
  tar xvfz data.tar.gz
  install -d $PKG/usr/lib/
  cp usr/lib/libkgsl.so.1.0 $PKG/usr/lib
}
